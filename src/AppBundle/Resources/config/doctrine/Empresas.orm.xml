<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Empresas" table="empresas">
    <id name="cempre" type="string" column="cempre" length="8">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nempre" type="string" column="nempre" length="80" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descrip" type="text" column="descrip" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="image" type="text" column="image" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="enviroment" type="string" column="enviroment" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="redirec" type="string" column="redirec" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default">N</option>
      </options>
    </field>
    <field name="lnk" type="string" column="lnk" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
